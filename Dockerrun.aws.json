{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "src",
      "image": "docker.io/sjryall/multi-client",
      "hostname": "src",
      "essential": false,
      "portMappings": [
        {
          "hostPort": 8080,
          "containerPort": 8080
        }
      ],
      "links": ["server"],
      "memory": 128
    },
    {
      "name": "server",
      "image": "docker.io/sjryall/multi-server",
      "hostname": "server",
      "essential": false,
      "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 8000
        }
      ],
      "memory": 128
    }
  ]
}